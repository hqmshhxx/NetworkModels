<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>social</display-name>
  <servlet>
    <servlet-name>preferentialAttachment</servlet-name>
    <servlet-class>servlet.PreferentialAttachment</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>preferentialAttachment</servlet-name>
    <url-pattern>/preferentialAttachmentShow</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>smallWorld</servlet-name>
    <servlet-class>servlet.SmallWorld</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>smallWorld</servlet-name>
    <url-pattern>/smallWorldShow</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>randomGraph</servlet-name>
    <servlet-class>servlet.RandomGraph</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>randomGraph</servlet-name>
    <url-pattern>/randomGraphShow</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>statistics</servlet-name>
    <servlet-class>servlet.Statistics</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>statistics</servlet-name>
    <url-pattern>/statisticsShow</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>